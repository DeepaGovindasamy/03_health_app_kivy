#:import button kivymd.uix.button
#:import label kivymd.uix.label
#:import boxlayout kivymd.uix.boxlayout

<HomeScreen>:
    BoxLayout:
        orientation: 'vertical'
        MDIconButton:
            icon: "home"
            pos_hint: {"center_x": .5}
            user_font_size: "80sp"
            theme_text_color: "Custom"
            text_color: app.theme_cls.primary_light
        MDLabel:
            text: "HOME Screen for Start "
            halign: "center"            
        

        StackLayout: 
            orientation: "lr-bt"
            padding: 20
            spacing: 20



            MDIconButton:
                icon: "account"
                text: "push"
                canvas.before:
                    Color:
                        rgb: app.theme_cls.primary_color
                    Rectangle:
                        size: self.size
                        pos: self.pos
            MDIconButton:
                icon: "account"
                text: "push"
            MDIconButton:
                icon: "account"
                text: "push"
                canvas.before:
                    Color:
                        rgb: app.theme_cls.primary_color
                    Ellipse:
                        size: 50, 50
                        pos: self.pos
                        
                        

